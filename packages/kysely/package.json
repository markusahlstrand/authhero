{
  "name": "@authhero/kysely-adapter",
  "private": false,
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/",
    "tag": "latest"
  },
  "homepage": "https://authhero.net",
  "repository": {
    "type": "git",
    "url": "https://github.com/markusahlstrand/authhero"
  },
  "version": "10.93.0",
  "files": [
    "dist"
  ],
  "main": "dist/kysely-adapter.cjs",
  "module": "dist/kysely-adapter.mjs",
  "types": "dist/kysely-adapter.d.ts",
  "exports": {
    ".": {
      "require": "./dist/kysely-adapter.cjs",
      "import": "./dist/kysely-adapter.mjs",
      "types": "./dist/kysely-adapter.d.ts"
    }
  },
  "scripts": {
    "build": "tsc && vite build && dts-bundle-generator --config ./dts-bundle-generator.config.ts",
    "test": "vitest run",
    "export-sql": "npx tsx scripts/export-sql.ts",
    "export-sql:d1": "npx tsx scripts/export-sql.ts --d1",
    "export-sql:combined": "npx tsx scripts/export-sql.ts --split",
    "export-sql:squash": "npx tsx scripts/export-sql.ts --squash"
  },
  "devDependencies": {
    "@hono/zod-openapi": "^0.19.2",
    "@rollup/plugin-commonjs": "^28.0.1",
    "@rollup/plugin-node-resolve": "^15.3.0",
    "@types/node": "^22.9.1",
    "better-sqlite3": "^11.10.0",
    "dts-bundle-generator": "^9.5.1",
    "hono": "^4.6.11",
    "kysely-bun-sqlite": "^0.3.2",
    "kysely-planetscale": "^1.5.0",
    "typescript": "^5.6.3",
    "vite": "^5.4.11",
    "vitest": "^2.1.5"
  },
  "dependencies": {
    "@authhero/adapter-interfaces": "workspace:*",
    "kysely": "^0.27.4",
    "nanoid": "^5.0.8",
    "ulid": "^2.3.0"
  },
  "peerDependencies": {
    "@hono/zod-openapi": "^0.19.2",
    "hono": "^4.6.8",
    "kysely-bun-sqlite": "^0.3.2",
    "kysely-planetscale": "^1.5.0"
  }
}
