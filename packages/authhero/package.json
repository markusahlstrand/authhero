{
  "name": "authhero",
  "version": "0.255.1",
  "files": [
    "dist"
  ],
  "main": "dist/authhero.cjs",
  "module": "dist/authhero.mjs",
  "types": "dist/authhero.d.ts",
  "exports": {
    ".": {
      "types": "./dist/authhero.d.ts",
      "require": "./dist/authhero.cjs",
      "import": "./dist/authhero.mjs"
    },
    "./styles": {
      "import": "./dist/tailwind.css",
      "require": "./dist/tailwind.css"
    },
    "./shadcn-styles": {
      "import": "./dist/shadcn-ui.css",
      "require": "./dist/shadcn-ui.css"
    }
  },
  "scripts": {
    "dev": "bun --watch src/bun.ts",
    "build": "pnpm build:tailwind && pnpm build:shadcn && pnpm build:client && tsc && vite build && dts-bundle-generator --config ./dts-bundle-generator.config.ts",
    "build:tailwind": "node build-tailwind.js",
    "build:shadcn": "node build-shadcn.js",
    "build:client": "vite build --mode client && node build-client.js",
    "build:vite": "vite build",
    "start": "pnpm build && node dist/index.js",
    "test": "vitest run",
    "pull-translations": "i18nexus pull --path ./src/locales",
    "watch-translations": "i18nexus listen --path ./src/locales",
    "storybook": "pnpm build:tailwind && pnpm build:shadcn && storybook dev -p 6006",
    "build-storybook": "pnpm build:tailwind && pnpm build:shadcn && storybook build -o ../../apps/docs/public/storybook"
  },
  "devDependencies": {
    "@ape-egg/tailwind-rows-columns": "^1.0.2",
    "@authhero/kysely-adapter": "workspace:^",
    "@hono/zod-openapi": "^0.19.2",
    "@storybook/react": "^9.1.10",
    "@storybook/react-vite": "^9.1.10",
    "@types/node": "^22.9.1",
    "@types/react": "^19.1.3",
    "@types/react-dom": "^19.1.3",
    "autoprefixer": "10.4.18",
    "better-sqlite3": "^11.5.0",
    "cssnano": "6.0.5",
    "dts-bundle-generator": "^9.5.1",
    "fast-xml-parser": "^4.5.1",
    "hono": "^4.8.7",
    "i18nexus-cli": "^3.8.2",
    "kysely": "^0.27.4",
    "postcss": "8.4.36",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "rollup-plugin-visualizer": "^6.0.4",
    "storybook": "^9.1.10",
    "tailwindcss": "3.4.1",
    "typescript": "^5.6.3",
    "vite": "^5.4.11",
    "vite-plugin-dts": "^4.3.0",
    "vitest": "^2.1.5"
  },
  "dependencies": {
    "@authhero/adapter-interfaces": "workspace:^",
    "@authhero/saml": "workspace:^",
    "@peculiar/x509": "^1.12.3",
    "arctic": "3.1.2",
    "bcryptjs": "^2.4.3",
    "check-password-strength": "^2.0.10",
    "classnames": "^2.5.1",
    "i18next": "^24.2.0",
    "libphonenumber-js": "^1.12.8",
    "nanoid": "^5.0.8",
    "oslo": "^1.2.1"
  },
  "peerDependencies": {
    "@hono/zod-openapi": "^0.19.2",
    "hono": "^4.8.7"
  }
}
