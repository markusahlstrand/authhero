{
  "name": "@authhero/drizzle",
  "private": false,
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/",
    "tag": "latest"
  },
  "homepage": "https://authhero.net",
  "repository": {
    "type": "git",
    "url": "https://github.com/markusahlstrand/authhero"
  },
  "version": "0.21.0",
  "files": [
    "dist",
    "src/schema",
    "drizzle"
  ],
  "main": "dist/drizzle.cjs",
  "module": "dist/drizzle.mjs",
  "types": "dist/drizzle.d.ts",
  "exports": {
    ".": {
      "require": "./dist/drizzle.cjs",
      "import": "./dist/drizzle.mjs",
      "types": "./dist/drizzle.d.ts"
    },
    "./schema/sqlite": {
      "import": "./src/schema/sqlite/index.ts",
      "types": "./src/schema/sqlite/index.ts"
    }
  },
  "scripts": {
    "build": "tsc && vite build",
    "db:generate": "drizzle-kit generate",
    "db:push": "drizzle-kit push"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^26.0.1",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@types/node": "^20.14.9",
    "drizzle-kit": "^0.31.4",
    "typescript": "^5.5.2",
    "vite": "^5.3.2"
  },
  "dependencies": {
    "@authhero/adapter-interfaces": "workspace:*",
    "drizzle-orm": "^0.44.2"
  }
}
